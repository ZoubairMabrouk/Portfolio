{
  "name": "zoubair-portfolio",
  "version": "1.0.0",
  "description": "Professional Portfolio - Zoubair Mabrouk",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
